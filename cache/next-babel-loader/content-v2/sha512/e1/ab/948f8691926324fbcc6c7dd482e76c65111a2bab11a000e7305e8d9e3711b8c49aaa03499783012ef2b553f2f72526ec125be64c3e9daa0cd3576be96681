splice(action.payload.index, 1, action.payload.datum)\n    case 'del_card':\n      return [...state].splice(action.payload.index, 1)\n    default:\n      return state\n  }\n}\n\nexport default function Home() {\n  const [cardData, dispatch] = useReducer(reducer, getData())\n  console.log('cardData: ', cardData)\n\n  const [openedCardIndex, setOpenedCardIndex] = useState(null)\n  const { isOpen, onOpen, onClose } = useDisclosure()\n  const [siteName, setSiteName] = useState('')\n  const [url, setUrl] = useState('')\n\n  useEffect(() => {\n    // 主页刷新前存入 localStorage 做持久化处理\n    return () => setData(cardData)\n  }, [cardData])\n\n  // const modalProps = {\n  //   isOpen,\n  //   onClose,\n  //   dispatch,\n  //   href: openedCardIndex === -1 ? null : cardData[openedCardIndex].href,\n  //   text: openedCardIndex === -1 ? null : cardData[openedCardIndex].text,\n  //   index: openedCardIndex,\n  // }\n  useEffect(() => {\n    if (openedCardIndex !== null) {\n      onOpen()\n      console.log('openedCardIndex: ', openedCardIndex)\n      if (openedCardIndex >= 0) {\n        setUrl(cardData[openedCardIndex].href)\n        setSiteName(cardData[openedCardIndex].text)\n      }\n    } else {\n      onClose()\n    }\n  }, [openedCardIndex])\n\n  const handleSubmit = () => {\n    if (siteName.trim() === '' || url.trim() === '') {\n      return\n    }\n    const index = openedCardIndex\n    const datum = { href: url, text: siteName }\n    if (index >= 0) {\n      console.log('正在修改现有数据')\n      dispatch({ type: 'edit_card', payload: { index, datum } })\n    } else {\n      console.log('正在添加新数据')\n      dispatch({ type: 'add_card', payload: { datum } })\n      setSiteName('')\n      setUrl('')\n    }\n    setOpenedCardIndex(null)\n  }\n\n  const handleDelete = () => {\n    console.log('正在删除数据')\n    dispatch({ type: 'del_card', payload: { index } })\n    setOpenedCardIndex(null)\n  }\n\n  return (\n    <>\n      <HeadContent text=\"New Tab\" />\n      <Center h=\"100vh\" maxW=\"800px\" w=\"100%\" mx=\"auto\" pb={20}>\n        <VStack>\n          <GoogleHeading />\n          <SearchBar />\n          <Wrap spacing=\"20px\" justify=\"center\">\n            {cardData &&\n              cardData.map((c, i) => (\n                <Card key={i + ''} {...c} index={i} onOpen={onOpen} setOpenedCardIndex={setOpenedCardIndex} />\n              ))}\n            <Card onOpen={onOpen} setOpenedCardIndex={setOpenedCardIndex} />\n            <FormatPlaceholder />\n          </Wrap>\n        </VStack>\n      </Center>\n      <Modal isOpen={isOpen} onClick={onClose} motionPreset=\"slideInBottom\" isCentered>\n        <ModalOverlay />\n        <ModalContent>\n          <ModalHeader>Add shortcut</ModalHeader>\n          <ModalBody pb={6}>\n            <FormControl>\n              <FormLabel>Website Name</FormLabel>\n              <Input placeholder=\"Website Name\" value={siteName} onChange={(e) => setSiteName(e.target.value)} />\n            </FormControl>\n\n            <FormControl mt={4}>\n              <FormLabel>URL</FormLabel>\n              <Input placeholder=\"URL\" value={url} onChange={(e) => setUrl(e.target.value)} />\n            </FormControl>\n          </ModalBody>\n\n          <ModalFooter>\n            <Flex w=\"100%\">\n              {openedCardIndex !== -1 && openedCardIndex !== null && (\n                <Button colorScheme=\"red\" mr={3} onClick={handleDelete}>\n                  Delete\n                </Button>\n              )}\n              <Spacer />\n              <Box>\n                <Button colorScheme=\"blue\" mr={3} onClick={handleSubmit}>\n                  Save\n                </Button>\n                <Button onClick={() => setOpenedCardIndex(null)}>Cancel</Button>\n              </Box>\n            </Flex>\n          </ModalFooter>\n        </ModalContent>\n      </Modal>\n    </>\n  )\n}\n\nconst FormatPlaceholder = () => (\n  <>\n    <EmptyPlaceHolderCard />\n    <EmptyPlaceHolderCard />\n    <EmptyPlaceHolderCard />\n    <EmptyPlaceHolderCard />\n    <EmptyPlaceHolderCard />\n  </>\n)\n"]},"metadata":{},"sourceType":"module"}