] = useState(cardData)\n  const [url, setUrl] = useState(href || '')\n  const handleSubmit = () => {\n    if (siteName.trim() === '' || url.trim() === '') {\n      return\n    }\n    const datum = { href: url, text: siteName }\n    const newData = [...cardData]\n    if (index !== undefined && index !== null) {\n      // 正在修改现有数据\n      console.log('正在修改现有数据')\n      setNewCardsData(newData.splice(index, 1, datum))\n      console.log(newData)\n    } else {\n      // 正在添加新数据\n      console.log('正在添加新数据')\n      newData.push(datum)\n      setNewCardsData(newData)\n      setSiteName('')\n      setUrl('')\n    }\n    setCardData(newCardsData)\n    onClose()\n  }\n\n  const handleDelete = () => {\n    const newData = [...cardData]\n    newData.splice(index, 1)\n    setNewCardsData(newData)\n    onClose()\n  }\n\n  useEffect(() => () => {\n    return setCardData(newCardsData)\n  })\n\n  return (\n    <Modal isOpen={isOpen} isCentered>\n      <ModalOverlay />\n      <ModalContent>\n        <ModalHeader>Add shortcut</ModalHeader>\n        <ModalBody pb={6}>\n          <FormControl>\n            <FormLabel>Website Name</FormLabel>\n            <Input value={siteName} placeholder=\"Website Name\" onChange={(e) => setSiteName(e.target.value)} />\n          </FormControl>\n\n          <FormControl mt={4}>\n            <FormLabel>URL</FormLabel>\n            <Input value={url} placeholder=\"URL\" onChange={(e) => setUrl(e.target.value)} />\n          </FormControl>\n        </ModalBody>\n\n        <ModalFooter>\n          <Flex w=\"100%\">\n            {index !== undefined && (\n              <Button colorScheme=\"red\" mr={3} onClick={handleDelete}>\n                Delete\n              </Button>\n            )}\n            <Spacer />\n            <Box>\n              <Button colorScheme=\"blue\" mr={3} onClick={handleSubmit}>\n                Save\n              </Button>\n              <Button onClick={onClose}>Cancel</Button>\n            </Box>\n          </Flex>\n        </ModalFooter>\n      </ModalContent>\n    </Modal>\n  )\n}\n\nexport default AddOrEditModal\n"]},"metadata":{},"sourceType":"module"}