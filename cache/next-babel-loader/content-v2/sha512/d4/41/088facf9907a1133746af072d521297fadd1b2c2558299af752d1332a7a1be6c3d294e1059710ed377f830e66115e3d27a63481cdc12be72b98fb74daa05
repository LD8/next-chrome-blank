        <FormatPlaceholder />\n          </Wrap>\n        </VStack>\n      </Center>\n      <Modal isOpen={isOpen} isCentered onClick={onClose} motionPreset=\"slideInBottom\">\n        <ModalOverlay />\n        <ModalContent>\n          <ModalHeader>Add shortcut</ModalHeader>\n          <ModalBody pb={6}>\n            <FormControl>\n              <FormLabel>Website Name</FormLabel>\n              <Input value={siteName} placeholder=\"Website Name\" onChange={(e) => setSiteName(e.target.value)} />\n            </FormControl>\n\n            <FormControl mt={4}>\n              <FormLabel>URL</FormLabel>\n              <Input value={url} placeholder=\"URL\" onChange={(e) => setUrl(e.target.value)} />\n            </FormControl>\n          </ModalBody>\n\n          <ModalFooter>\n            <Flex w=\"100%\">\n              {index !== -1 && (\n                <Button colorScheme=\"red\" mr={3} onClick={handleDelete}>\n                  Delete\n                </Button>\n              )}\n              <Spacer />\n              <Box>\n                <Button colorScheme=\"blue\" mr={3} onClick={handleSubmit}>\n                  Save\n                </Button>\n                <Button onClick={onClose}>Cancel</Button>\n              </Box>\n            </Flex>\n          </ModalFooter>\n        </ModalContent>\n      </Modal>\n    </>\n  )\n}\n\nconst FormatPlaceholder = () => (\n  <>\n    <EmptyPlaceHolderCard />\n    <EmptyPlaceHolderCard />\n    <EmptyPlaceHolderCard />\n    <EmptyPlaceHolderCard />\n    <EmptyPlaceHolderCard />\n  </>\n)\n"]},"metadata":{},"sourceType":"module"}